{
  "expo": {
    "name": "BokjaApp",
    "slug": "bokja-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main_icon/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/main_icon/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFCC02"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main_icon/icon.png",
        "backgroundColor": "#FFCC02"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.synergy_dgu.bokjaapp",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "CAMERA",
        "POST_NOTIFICATIONS",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "USE_FULL_SCREEN_INTENT"
      ]
    },
    "web": {
      "favicon": "./assets/images/main_icon/icon.png"
    },
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "e1978ac7-7d67-45fa-bf17-72f7e988c73e"
        }
    }
  }
}
